{
  "name": "diegesis",
  "version": "1.0.0",
  "type": "module",
  "description": "React + TypeScript + Vite + Github Actions starter template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickporto/diegesis.git"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "template",
    "ghactions",
    "vitest"
